<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/user-list-style.css">
    <title>Users list</title>
</head>
<body>
    <div class="main">
        <table>
            <caption>Liste de tous les utilisateurs</caption>
            <thead>
                <tr>
                    <th>#Id</th>
                    <th>Nom & Prenom</th>
                    <th>Email</th>
                    <th colspan="2">Action</th>
                </tr>
            </thead>
            <tbody>
                <% data.forEach(user => {%>
                    <tr>
                        <td><%= user.dataValues.id %></td>
                        <td><%= user.dataValues.noms %></td>
                        <td><%= user.dataValues.email %></td>
                        <td><a class="update" href="">Modifier</a></td>
                        <td><a class="delete" href="">Supprimer</a></td>
                    </tr>
                <% }) %>
                
            </tbody>
        </table>
    </div>
</body>
</html>